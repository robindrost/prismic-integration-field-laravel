{
  "name": "robindrost/prismic-integration-field-laravel",
  "authors": [
    {
      "name": "Robin Drost",
      "email": "robindrost@gmail.com",
      "homepage": "https://github.com/robindrost"
    }
  ],
  "license": "MIT",
  "description": "A package that allows you to convert your models or collection of models to an Prismic integration field ready array.",
  "require": {
    "php": "^7.1 || ^8.0",
    "illuminate/support": "^7.0 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
    "psr-4": {
      "RobinDrost\\PrismicIntegrationField\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RobinDrost\\PrismicIntegrationField\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RobinDrost\\PrismicIntegrationField\\Providers\\ServiceProvider"
      ]
    }
  }
}
